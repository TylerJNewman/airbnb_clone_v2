    "@next/bundle-analyzer": "^9.1.4",
    "antd": "4.3.0",
    "apollo-server-micro": "^2.16.1",
    "babel-plugin-import": "1.13.0",
    "deepmerge": "^4.2.2",
    "esm": "^3.2.25",
    "googleapis": "^65.0.0",
    "graphql": "^15.4.0",
    "mongodb": "^3.6.3",
    "next": "latest",
    "react": "^16.7.0",
